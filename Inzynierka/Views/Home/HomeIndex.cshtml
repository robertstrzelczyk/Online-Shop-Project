@model IEnumerable<Inzynierka.Models.ShopViewModel>
@{
    ViewBag.Title = "HomeIndex";
}
<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
   @* <title>@ViewBag.Title - My ASP.NET Application</title>*@
    <link href="~/Content/Site.css" rel="stylesheet" type="text/css" />
    <link href="~/Content/bootstrap.min.css" rel="stylesheet" type="text/css" />
    <link href="~/Content/bootstrap.css" rel='stylesheet' type='text/css' />
    <link href="~/Content/style.css" rel="stylesheet" type="text/css" media="all" />
    <link href="~/Content/HomeStyle.css"  rel="stylesheet" type="text/css" media="all" />
    <script src="~/Scripts/modernizr-2.6.2.js"></script>
    <script type="application/x-javascript"> addEventListener("load", function() { setTimeout(hideURLbar, 0); }, false); function hideURLbar(){ window.scrollTo(0,1); } </script>
    <!--webfont-->
    <link href='http://fonts.googleapis.com/css?family=Lato:100,300,400,700,900,100italic,300italic,400italic,700italic,900italic' rel='stylesheet' type='text/css'>
    <script src="~/Scripts/scripts.js" type="text/javascript"></script>
</head>
<body>
    <div class="container">
        <div class="container-fluid">
            <div class="row" id="row1">
                <div id="Asus" class="col-lg-4">             
                    <a href="@Url.Action("ItemsIndex", "Items", new { Manufacturer = "Asus" })">
                        <img src="~/Content/images/logo/asus_logo.png" width="200" height="70" />
                    </a>              
                       </div>

                <div id="MSI" class="col-lg-4">
                    <a href="@Url.Action("ItemsIndex", "Items", new { Manufacturer = "MSI"})">
                        <img src="~/Content/images/logo/MSI_logo_for_share2.png" width="200" height="70" />
                    </a>
                </div>
                <div id="Dell" class="col-lg-4">
                    <a href="@Url.Action("ItemsIndex", "Items", new { Manufacturer = "Dell"})">
                        <img src="~/Content/images/logo/dell-old-logo.jpg" width="200" height="70" />
                    </a>
                </div>
            </div>
            <div class="row" id="row2">
                <div id="Acer" class="col-lg-4">
                    <a href="@Url.Action("ItemsIndex", "Items", new { Manufacturer = "Acer"})">
                        <img src="~/Content/images/logo/acer.png" width="200" height="70" />
                    </a>
                </div>
                <div id="Lenovo" class="col-lg-4">
                    <a href="@Url.Action("ItemsIndex", "Items", new { Manufacturer = "Lenovo"})">
                        <img src="~/Content/images/logo/lenovo.png" width="200" height="70" />
                    </a>
                </div>
                <div id="HP" class="col-lg-4">
                    <a href="@Url.Action("ItemsIndex", "Items", new { Manufacturer = "HP"})">                      
                        <img src="~/Content/images/logo/hp-logo.png" ` width="200" height="70"/>
                    </a>           
                </div>
            </div>
            <div class="row" id="row3">
                <div id="Sony" class="col-lg-4">
                    <a href="@Url.Action("ItemsIndex", "Items", new { Manufacturer = "MSI" })">
                        <img src="~/Content/images/logo/sony_logo_PNG7.png" width="200" height="70" />
                    </a>
                    </div>
                    <div id="Samsung" class="col-lg-4">
                        <a href="@Url.Action("ItemsIndex", "Items", new { Manufacturer = "Toshiba"})">
                            <img src="~/Content/images/logo/TOSHIBA_Logo.png" width="200" height="70" />
                        </a>
                    </div>
                <div id="Apple" class="col-lg-4">
                    <a href="@Url.Action("ItemsIndex", "Items", new { Manufacturer = "Apple"})">
                        <img src="~/Content/images/logo/Apple-Logo-300x161.png" width="200" height="70" />
                    </a>
                </div>
            </div>
        </div>
    </div>

</body>
</html>
